#Fetching the latest verison of java
FROM openjdk:20

#setting the work directory 
WORKDIR /target

#copy the jar file into our target 
COPY ./login-register-0.0.1-SNAPSHOT.jar /target

#Expose port 8135
EXPOSE 8135

#Starting the application
CMD ["java", "-jar", "login-register-0.0.1-SNAPSHOT.jar"]